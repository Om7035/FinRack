# Database
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/finrack
DATABASE_URL_SYNC=postgresql://postgres:postgres@localhost:5432/finrack

# Redis
REDIS_URL=redis://localhost:6379/0

# RabbitMQ
RABBITMQ_URL=amqp://guest:guest@localhost:5672/

# MinIO (S3-compatible storage)
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET=finrack-receipts

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30
JWT_REFRESH_TOKEN_EXPIRE_DAYS=7

# CORS
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# Plaid (Banking Integration)
PLAID_CLIENT_ID=your_plaid_client_id
PLAID_SECRET=your_plaid_secret
PLAID_ENV=sandbox

# Groq (Fast LLM Inference)
GROQ_API_KEY=your_groq_api_key

# Anthropic Claude (Optional)
ANTHROPIC_API_KEY=your_anthropic_api_key

# OpenRouter (Optional fallback)
OPENROUTER_API_KEY=your_openrouter_api_key

# Ollama (Local LLM)
OLLAMA_BASE_URL=http://localhost:11434

# Twilio (SMS Notifications)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number

# SendGrid (Email)
SENDGRID_API_KEY=your_sendgrid_api_key
SENDGRID_FROM_EMAIL=noreply@finrack.com

# AWS (Optional - for Textract OCR)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1

# Application
APP_NAME=FinRack
APP_VERSION=0.1.0
DEBUG=True
LOG_LEVEL=INFO

# Security
ENCRYPTION_KEY=your-encryption-key-for-sensitive-data
